<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="functionPane" onMouseClicked="#handleFunctionClicked" prefHeight="180.0" prefWidth="200.0" style="-fx-background-color: #FFF3E0; -fx-background-radius: 20;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text fx:id="functionName" fill="#212121" layoutX="3.0" layoutY="99.0" lineSpacing="1.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" textAlignment="CENTER" wrappingWidth="200.0">
         <font>
            <Font name="Verdana" size="25.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
